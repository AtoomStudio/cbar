                    <p>Finn's: Golden Tavern</p>
                </div>
                <div class="headerScreen__btns">
                    <button class="btn__red btn__red--glow play-btn">
                        <span>¡Juega!</span>
                        <img src="img/icons/icon-play-white.svg" alt="">
                    </button>
                    <button class="btn__darkGrey demo-btn">
                        <span>Pruébalo</span>
                    </button>
                </div>
            </div>

            <%- include ('modules/aside-slots') %>
