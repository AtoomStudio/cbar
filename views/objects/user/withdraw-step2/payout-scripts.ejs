<script>
    function initUrlDataBtns() {
        var mpuBtns = document.querySelectorAll("[data-url]");

        mpuBtns.forEach((btn) => {
            btn.addEventListener("click", e => {
                e.preventDefault();
                mpu(e.currentTarget.dataset.url);
            });
        });
    }

    function initDisabledBtns() {
        var mpuBtns = document.querySelectorAll("[data-mpu-body]");

        mpuBtns.forEach((btn) => {
            btn.addEventListener("click", e => {
                e.preventDefault();
                mpu({ title: "Verifica el m√©todo de pago", body: e.currentTarget.dataset.mpuBody });
            });
        });
    }

    initUrlDataBtns();
</script>