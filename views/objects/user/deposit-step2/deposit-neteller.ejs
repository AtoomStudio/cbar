                            </svg>
                            <div class="item__type">Poker</div>
                            <div class="radio">
                                <span></span>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="depositDetail__select-content"></div>
                <div class="depositDetail__select-noBond">
                    <div class="item">
                        <input type="radio" name="bondType" id="">
                        <div class="item__wrapper">
                            <div class="item__type">No quiero bono</div>
                            <div class="radio">
                                <span></span>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="depositDetail__select-email">
                    <input type="email" name="" id="" placeholder="Introduce tu mail de Skrill">
                </div>
            </div>
            <div class="depositDetail__btn">
                <button class="btn__white depositStep2__backBtn" type='button' data-url="/mpu-deposit">
                    <img src="img/icons/icon-arrow-black.svg" alt="">
                    <span>ATRÁS</span>
                </button>
                <button class="btn__blue btn__icons" type="submit">
                    <img src="img/icons/icon-plus-btn.svg" alt="">
                    <span>DEPOSITAR</span>
                </button>
            </div>
        </form>
    </div>
</section>
<%- include ('deposit-step2-scripts') %>