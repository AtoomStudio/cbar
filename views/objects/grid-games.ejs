<ul class="gridGames container">
    <% let i = 1; %>
        <% data.forEach( e=> { %>
            <li class="gridGames__item mix <%= e.general %> <%= e.proveedor %> <%= e.tipo %> <%= e.apuesta %> <%= e.lineas %> <%= e.juego %>">
                <img src="img/img-grid-casino-<%= i %>.jpg" alt="">
                <div class="gridGames__item-title">
                    <p class="">
                        <%= e.title %>
                    </p>
                    <div class="is-favourite">
                        <svg xmlns="http://www.w3.org/2000/svg" width="28" height="28">
                            <g transform="translate(6.977 -2.109)">
                                <circle data-name="Elipse 3" cx="14" cy="14" r="14" transform="translate(-6.977 2.109)"
                                    fill="#13171d" />
                                <path data-name="Trazado 99"
                                    d="M13.856 14.015a4 4 0 0 0-2.839-2.829 4.168 4.168 0 0 0-3.828 1.289.25.25 0 0 1-.34 0 4.168 4.168 0 0 0-3.829-1.289 4 4 0 0 0-2.838 2.829 3.9 3.9 0 0 0 1 3.868L6.7 22.971a.51.51 0 0 0 .68 0l5.528-5.108a3.868 3.868 0 0 0 .948-3.848Z"
                                    fill="#fff" opacity=".5" />
                            </g>
                        </svg>
                    </div>
                </div>
                <div class="gridGames__item-hover">
                    <a href="<%= e.link %>">
                        <button class="btn__red btn__red--glow">
                            <span>¡JUEGA!</span>
                            <img src="img/icon-play-white.svg" alt="">
                        </button>
                    </a>
                    <p>PRUÉBALO</p>
                </div>
            </li>
        <% i = i + 1; %>
    <% }) %>
</ul>