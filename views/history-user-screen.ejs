<%
    let nav = {
        data: '',
        docs: '',
        wallet: '',
        hist: 'nav-active',
        pref: '',
        supp: '',
    }
%>

<%- include ('layout/head', {title: "Mi Historial" , css: "user" }) %>

<%- include ('layout/header', { mode: "header--light"}) %>

<main class="userScreen">

    <%- include ('objects/user/user-menu', { nav: nav }) %>

    <%- include ('objects/user/user-menu-mobile', { nav: nav }) %>

    <section class="userContent has-header">
        <div class="userContent__block">
            <%- include ('objects/user/history') %>
        </div>
    </section>
</main>

<main class="withdrawScreen">

    <div class="withdraw">

        <section class="withdraw__header">
            <img src="img/user/logo/logo-casino-dark.png" alt="">
            <img src="img/user/icon/icon-menu-close.svg" alt="" class="withdraw__btn-close">
        </section>

        <div class="withdraw__block">
            <%- include ('objects/user/withdraw') %>
        </div>

        <div class="withdraw__block">
            <%- include ('objects/user/withdraw-detail') %>
        </div>

        <div class="withdraw__block">
            <%- include ('objects/user/withdraw-ok') %>
        </div>

    </div>

</main>

<main class="depositScreen">

    <div class="deposit">

        <section class="deposit__header">
            <img src="img/user/logo/logo-casino-dark.png" alt="">
            <img src="img/user/icon/icon-menu-close.svg" alt="" class="deposit__btn-close">
        </section>

        <div class="deposit__block">
            <%- include ('objects/user/deposit') %>
        </div>

        <div class="deposit__block">
            <%- include ('objects/user/deposit-detail') %>
        </div>

        <div class="deposit__block">
            <%- include ('objects/user/deposit-ok') %>
        </div>

    </div>

</main>

<%- include ('layout/scripts', { script: 'history'})%>

